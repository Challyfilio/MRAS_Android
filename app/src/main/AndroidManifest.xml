<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.david.mras">

    <!-- 允许连接互联网并了解当前的网络状态 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lollipop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lollipop"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".ShowActivity" />
        <activity android:name=".ChangeActivity" />
        <activity android:name=".InsertActivity"></activity>
    </application>

</manifest>